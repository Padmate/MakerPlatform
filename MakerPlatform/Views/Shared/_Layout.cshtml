    <link href="~/Content/bootstrap.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="~/Scripts/slick/css/style.css" rel="stylesheet">
    <link href="~/Scripts/slick/slick/slick.css" rel="stylesheet">
    <link href="~/Content/animate.css" rel="stylesheet">
    <link href="~/Content/creative.css" rel="stylesheet">
    <link href="~/Content/agency.css" rel="stylesheet">
    <link href="~/Content/ribbon.css" rel="stylesheet">
    <link href="~/Content/SuspendedNav.css" rel="stylesheet">
    <link href="~/Scripts/jqzoom-2.3/css/jquery.jqzoom.css" rel="stylesheet">
    <link href="~/Scripts/superslider/superslider.css" rel="stylesheet">

    <link href="~/Content/buttons.css" rel="stylesheet">
    <link href="~/Content/bootstrap-datetimepicker.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Scripts/loading/showLoading.css">
    <link href="~/Content/system.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link rel="stylesheet" href="~/fonts/font-awesome/css/font-awesome.min.css">
