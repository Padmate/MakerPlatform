@Html.Partial("_CommonHeader")
<style>
    .desbg {
        background-color: rgba(0, 0, 0, 0.09);
        height: 150px;
        padding: 50px 50px;
        margin-top: 70px;
    }
</style>
<section style="padding:100px;">

    <div class="container">
        <div class="row">
            <div class="col-md-8 col-sm-8">
                <div id="zlgh" class="carousel slide">
                    <!-- 轮播（Carousel）指标 -->
                    <ol class="carousel-indicators">
                        <li data-target="#zlgh" data-slide-to="0" class="active"></li>
                        <li data-target="#zlgh" data-slide-to="1"></li>
                        <li data-target="#zlgh" data-slide-to="2"></li>
                    </ol>
                    <!-- 轮播（Carousel）项目 -->
                    <div class="carousel-inner">
                        <div class="item active">
                            <img src="~/img/Service/" style="height:300px; width:100%" alt="Second slide">

                        </div>
                        <div class="item">
                            <img src="~/img/Service/" style="height:300px;width:100%" alt="First slide">

                        </div>
                        <div class="item">
                            <img src="~/img/Service/" style="height:300px;width:100%;" alt="Third slide">
                        </div>
                    </div>
                    <!-- 轮播（Carousel）导航 -->
                    <a class="carousel-control left" href="#zlgh"
                       data-slide="prev">&lsaquo;</a>
                    <a class="carousel-control right" href="#zlgh"
                       data-slide="next">&rsaquo;</a>
                </div>
            </div>
            <div class="col-md-4 col-sm-4">
                <h2>创新创业战略规划</h2>
            </div>

        </div>
        
    </div>
</section>
<div class="row desbg text-center">
    <h2>企业教练：提供教练式管理、辅导</h2>
</div>
<div class="row desbg" style="height:250px;">
    <h2>顾问培训</h2>
    <ul>
        <li><span class="customerFont">提供产品相关的外观、新型实用、发明专利规划的咨询辅导工作</span></li>
        <li><span class="customerFont">提供项目可行性，运作管理、品质、交期、成本、风险管控等方面工作的咨询辅导</span></li>

    </ul>
</div>

<!-- Team Section -->
<section id="team" class="bg-light-gray">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">产学研专家、工程师</h2>
                <h3 class="section-subheading text-muted">&nbsp;</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-4">
                <div class="team-member">
                    <img src="~/img/team/1.jpg" class="img-responsive img-circle" alt="">
                    <h4>Kay Garland</h4>
                    <p class="text-muted">导师资料介绍</p>
                    
                </div>
            </div>
            <div class="col-sm-4">
                <div class="team-member">
                    <img src="~/img/team/2.jpg" class="img-responsive img-circle" alt="">
                    <h4>Larry Parker</h4>
                    <p class="text-muted">导师资料介绍</p>
                    
                </div>
            </div>
            <div class="col-sm-4">
                <div class="team-member">
                    <img src="~/img/team/3.jpg" class="img-responsive img-circle" alt="">
                    <h4>Diana Pertersen</h4>
                    <p class="text-muted">导师资料介绍</p>
                    
                </div>
            </div>
        </div>
       
    </div>
</section>
<section style="padding:100px;">

    <div class="container">
        <h1 class="text-center">协 会</h1>
        <div class="row">
            <div class="col-md-4 col-sm-4">
                <img src="~/img/Service/TutorPlatform/厦门市众创空间产业协会.jpg" style="height:300px;width:100%" alt="协会图片" />
                
            </div>
            <div class="col-md-4 col-sm-4">
                <img src="~/img/Service/TutorPlatform/硅谷高创会.png" style="height:300px;width:100%" alt="协会图片" />
                
            </div>
            <div class="col-md-4 col-sm-4">
                <img src="~/img/Service/TutorPlatform/11gy_head.jpg" style="height:300px;width:100%" alt="协会图片" />
               
            </div>

        </div>
    </div>
</section>

<script>
    $(function () {
        //选中“服务”
        $('ul.nav > li:eq(2)').addClass('active');
        
        $('#zlgh').carousel({
            interval: 2000,
            pause: 'cycle'
        });
    });
</script>